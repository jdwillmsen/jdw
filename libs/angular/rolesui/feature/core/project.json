{
  "name": "angular-rolesui-feature-core",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/angular/rolesui/feature/core/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [
    "type:feature",
    "scope:rolesui",
    "framework:angular",
    "library:feature"
  ],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/angular/rolesui/feature/core/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "libs/angular/rolesui/feature/core/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "rolesui:build"
      }
    }
  }
}
